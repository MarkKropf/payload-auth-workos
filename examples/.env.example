# WorkOS Configuration
# Get these from https://dashboard.workos.com

# WorkOS Client ID
WORKOS_CLIENT_ID=client_your_client_id_here

# WorkOS API Key (also called Client Secret)
WORKOS_API_KEY=sk_your_api_key_here

# Cookie Password (must be at least 32 characters)
# Generate with: openssl rand -base64 32
WORKOS_COOKIE_PASSWORD=your_32_character_minimum_secure_password_here

# Payload CMS Configuration
PAYLOAD_SECRET=your_payload_secret_here
PAYLOAD_PUBLIC_SERVER_URL=http://127.0.0.1:3000

# Database Configuration (example for MongoDB)
DATABASE_URI=mongodb://127.0.0.1:27017/payload-workos
